{
    "name": "@sedoo/unidoo",
    "version": "0.0.62",
    "private": false,
    "scripts": {
        "build": "vue-cli-service build --target lib --name unidoo src/plugin.js",
        "lint": "vue-cli-service lint --fix && vue-cli-service lint dev --fix",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs"
    },
    "comments": {
        "scripts": "The npm run build is used by github for publishing. It musn't be removed"
    },
    "files": [
        "dist",
        "src/**/*"
    ],
    "dependencies": {
        "ol": "^6.8.1",
        "v-tooltip": "^2.1.3",
        "vue-axios": "3.2.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "4.5.7",
        "@vue/cli-plugin-eslint": "4.5.7",
        "@vue/cli-service": "4.5.7",
        "@vue/eslint-config-standard": "5.1.2",
        "axios": "0.19.2",
        "babel-eslint": "10.1.0",
        "core-js": "3.6.5",
        "eslint": "7.10.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-promise": "4.2.1",
        "eslint-plugin-standard": "4.0.0",
        "eslint-plugin-vue": "7.0.1",
        "sass": "1.27.0",
        "sass-loader": "10.0.2",
        "vue": "2.6.14",
        "vue-cli-plugin-vuetify": "2.0.7",
        "vue-json-tree": "0.4.3",
        "vue-json-tree-view": "2.1.6",
        "vue-template-compiler": "2.6.14",
        "vuepress": "1.8.2",
        "vuetify": "2.3.13",
        "vuetify-loader": "1.6.0",
        "webpack-cdn-upload-plugin": "2.0.2"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-console": 0,
            "no-debugger": 0,
            "no-trailing-spaces": 0,
            "comma-dangle": 0,
            "indent": 0
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "description": "Commons components to simplify vuetify based application developement",
    "main": "./dist/unidoo.common.js",
    "module": "./dist/unidoo.umd.min.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sedoo/unidoo.git"
    },
    "author": "SEDOO",
    "bugs": {
        "url": "https://github.com/sedoo/unidoo/issues"
    },
    "homepage": "https://github.com/sedoo/unidoo#readme"
}
